<script>
  function scroll(e, bool){
    var parent = e.parentNode
    var scrollDiv = parent.querySelector('.category-list')
    var temp = 0;
    var interval = setInterval(function(){
      if(temp++ < 78){
        if(bool){
          scrollDiv.scrollLeft = scrollDiv.scrollLeft + 2
        } else {
          scrollDiv.scrollLeft = scrollDiv.scrollLeft - 2
        }
      } else {
        clearInterval(interval)
      }
    }.bind(this),1)
  }
</script>
